
import java.util.*;
import java.util.stream.Collectors;


public class CompleteSetValidator implements NumberValidator {
    private static final Set<Integer> COMPLETE_SET = 
        Set.of(1, 2, 3, 4, 5, 6, 7, 8, 9);
    
    @Override
    public Set<Integer> findMissingNumbers(Set<Integer> presentNumbers) {
        return COMPLETE_SET.stream()
            .filter(num -> !presentNumbers.contains(num))
            .collect(Collectors.toSet());
    }
    
    @Override
    public boolean isValidSet(Set<Integer> numbers) {
        // CORRECT: A valid Sudoku set must have ALL numbers 1-9
        // This means no duplicates AND no missing numbers
        return numbers.size() == 9 && COMPLETE_SET.containsAll(numbers);
    }
}
